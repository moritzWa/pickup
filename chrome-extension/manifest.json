{
  "manifest_version": 3,
  "name": "Pickup URL Saver",
  "version": "1.0",
  "description": "Save the current URL to your queue",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "couch_and_lamp.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
