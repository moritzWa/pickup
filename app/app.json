{
  "expo": {
    "name": "Pickup",
    "slug": "pickup",
    "version": "1.0.0",
    "scheme": "pickup",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pickup.www"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "om.pickup.www"
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ]
    ]
  },
  "name": "pickup"
}
